    "eslint-plugin-react-hooks": "^4.6.0",
    "jsdom": "^27.0.0",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.4.13",
    "typescript": "^5.6.2",
    "vite": "^7.1.9",
